<% provide(:title, 'All users') %>
<div class="row justify-content-center">
    <div class="col-md-6">
        <h1>All users</h1>
        <%= will_paginate %>
            <ul class="users">
                <% @users.each do |user| %>
                    <%= render user %>
                <% end %>
            </ul>
        <%= will_paginate %>
    </div>
</div>